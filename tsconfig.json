{
  "compilerOptions": {
    "strict": false,
    "declaration": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "noImplicitAny": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "./.orion/build/app",
    "rootDir": "./app",
    "baseUrl": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}